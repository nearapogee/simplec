<!DOCTYPE html>
<html>
  <head>
    <title>Dummy</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="container">
      <ul class="list-inline">
        <li><%= link_to 'Dashboard', admin_root_url %></li>
        <li><%= link_to 'Subdomains', admin_subdomains_url %></li>
        <li><%= link_to 'Pages', admin_pages_url %></li>
        <li><%= link_to 'Documents', admin_documents_url %></li>
        <li class="pull-right"><%= link_to "Sign out", admin_session_path,  method: :delete, class: 'btn btn-xs btn-danger' %></li>
      </ul>
      <%= yield %>
    </div>
    <div class="container">
      <div class="footer">
        <ul class="list-inline">

          <!-- here or index.html.erb? -->
          <% @subdomains.each do |subdomain| %>
            <li><%= link_to simplec_url_for(subdomain) %></li>
          <% end %>
          
        </ul>
      </div>
    </div>
  </body>
</html>
